<!-- Headers -->
<p><%= locals.status %></p>
<% if (obj.headers) { %>
    <p>Headers</p>
    <%- include('./schema-table', { schema: obj.headers }); -%>
<% } %>
<!-- Body -->
<% if (obj.body) { %>
    <p>Body</p>
    <p>Content-Type: <%= obj.body.type %> </p>
    <%- include('./schema-table', { schema: obj.body.schema }); -%>
<% } %>
<!-- Example -->
<% if (obj.example) {  %>
    <p>Example</p>
    <p class="markdown"><%= obj.example %></p>
<% } %>
